<div class="page-header">
  <h1>Subjects <%= link_to "new subject", new_admin_subject_path, class: "btn btn-primary pull-right" %></h1>
</div>

  <table class="table">
    <thead>
      <th>Subject Name</th>
      <th>Delete Subject</th>
    </thead>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= link_to subject.name, admin_subject_path(subject) %></td>
        <td><%= link_to "delete", admin_subject_path(subject), method: :delete %></td>
      </tr>
    <% end %>
  </table>
